.container {
  padding: 0rpx;
  background-color: #f5f6f7;
  height: 100%;
}
.personinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  padding: 60px 0;
  background-color: #fff;
}

.photo {
  width: 90px;
  height: 90px;
  border-radius: 90px;
}

.nickname {
  margin-top: 15px;
  text-align: center;
  color: #696969;
}

.menus {
  width: 750rpx;
  background-color: #fff;
  font-size: 28rpx;
  font-family: "微软雅黑";
  margin-top: 15rpx;
  border-top: 1rpx solid #e5e5e5;
}

.menu {
  height: 45px;
  border-bottom: 1px solid #e5e5e5;
  padding: 0 10px;
  line-height: 45px;
  position: relative;
}

.iconright {
  position: absolute;
  width: 40rpx;
  height: 30rpx;
  right: 10rpx;
  top: 30rpx;
}

.loginout {
  height: 45px;
  line-height: 45px;
  font-size: 25rpx;
  /* font-weight: bold; */
  font-family: "微软雅黑";
  text-align: center;
  background-color: #fff;
  margin-top: 10px;
  border-bottom: 1px solid #e5e5e5;
  border-top: 1px solid #e5e5e5;
}

.login {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 750rpx;
  padding-top: 300rpx;
  margin: 0rpx;
  background-color: #fff;
}

.loginimage {
  width: 90rpx;
  height: 85rpx;
  margin-bottom: 15rpx;
}
