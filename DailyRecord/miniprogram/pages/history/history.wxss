.container {
  display: flex;
  flex-direction: column;
}
.title{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  height: 200rpx;
  width: 100%;
  background-color: greenyellow;
}
/* 开始结束日期 -*/
.date {
  display: flex;
  flex-direction: row;
  margin-top: 20rpx;
}

.startdate {
  display: flex;
  flex-direction: row;
}

.enddate {
  display: flex;
  flex-direction: row;
  margin-left: 20rpx;
}

/*明细列表*/
.body {
  margin-top: 20rpx;
  background-color: white;
}

.f-second {
  display: flex;
  flex-direction: row;
  margin: 10rpx 10rpx 0rpx 10rpx;
  padding: 5rpx;
  border-bottom: 1px solid #e9e9e9;
}

.f-second-text {
  margin-top: 10rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
