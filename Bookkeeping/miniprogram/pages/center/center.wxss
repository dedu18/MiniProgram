.container {
  display: flex;
  flex-direction: column;
}

.user {
  display: flex;
  flex-direction: row;
  height: 200rpx;
  background-color: #242424;
}

.headimage {
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
  overflow: hidden;
  margin-top: 70rpx;
  margin-left: 50rpx;
}

.menuline {
  height: 20rpx;
  background-color:#e0e0e0;
}

.menu {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.rightimage {
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  overflow: hidden;
}

.aboutimage {
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
  overflow: hidden;
  margin: 20rpx 0rpx 20rpx 50rpx;
}
.menulinebet {
  height: 5rpx;
  background-color: #e9e9e9;
}
